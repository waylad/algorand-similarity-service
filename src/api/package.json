{
  "name": "algorand-similarity-service-api",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "PORT=8080 NODE_ENV=development nodemon --watch src -e ts,tsx --exec 'ts-node' src/index.ts",
    "build": "PORT=3000 NODE_ENV=production tsc",
    "analyze": "PORT=3000 NODE_ENV=production tsc && npx source-map-explorer 'build/static/js/*.js'",
    "docker-create": "doctl k8s cluster create --region fra1 algorand-similarity-service-api --wait && doctl kubernetes cluster kubeconfig save algorand-similarity-service-api",
    "docker-build": "docker build -t neutrino/algorand-similarity-service-api . --no-cache",
    "docker-run": "docker run --rm -it -p 3000:3000 neutrino/algorand-similarity-service-api",
    "docker-push": "doctl registry login && docker tag neutrino/algorand-similarity-service-api registry.digitalocean.com/neutrino/algorand-similarity-service-api && docker push registry.digitalocean.com/temp/algorand-similarity-service-api",
    "docker-patch": "doctl registry kubernetes-manifest | kubectl --context do-fra1-algorand-similarity-service-api apply -f - && kubectl --context do-fra1-algorand-similarity-service-api patch serviceaccount default -p '{\"imagePullSecrets\": [{\"name\": \"registry-temp\"}]}'",
    "docker-secrets": "kubectl --context do-fra1-algorand-similarity-service-api create secret generic person --from-env-file=.env",
    "docker-deploy": "kubectl --context do-fra1-algorand-similarity-service-api apply -f kubernetesDeployment.json",
    "docker-serve": "kubectl --context do-fra1-algorand-similarity-service-api apply -f kubernetesService.json",
    "docker-restart": "kubectl --context do-fra1-algorand-similarity-service-api delete pods -l app=algorand-similarity-service-api",
    "docker-logs": "kubectl --context do-fra1-algorand-similarity-service-api logs --timestamps -l app=algorand-similarity-service-api",
    "docker-update": "yarn run docker-build && yarn run docker-push && yarn run docker-restart",
    "test": "jest --runInBand --verbose",
    "coverage": "jest --runInBand --collectCoverage"
  },
  "dependencies": {
    "@koa/cors": "^3.1.0",
    "@koa/router": "^10.1.1",
    "@sendgrid/mail": "^7.6.1",
    "@sentry/node": "^6.17.9",
    "@typegoose/typegoose": "^9.6.2",
    "axios": "^0.26.0",
    "bcryptjs": "^2.4.3",
    "chalk": "^5.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "dayjs": "^1.10.7",
    "dotenv": "^16.0.0",
    "exceljs": "^4.3.0",
    "jsonwebtoken": "^8.5.1",
    "koa": "^2.13.4",
    "koa-bodyparser": "^4.3.0",
    "koa-helmet": "^6.1.0",
    "koa-socket-2": "^2.0.0",
    "mongoose": "^6.2.2",
    "node-binance-api": "^0.13.1",
    "rate-limiter-flexible": "^2.3.6",
    "reflect-metadata": "^0.1.13",
    "socket.io": "^4.4.1",
    "speakingurl": "^14.0.1",
    "telegram-bot-api": "^2.0.1",
    "winston": "^3.6.0",
    "ws": "^8.5.0"
  },
  "devDependencies": {
    "@shelf/jest-mongodb": "^2.2.0",
    "@types/axios": "^0.14.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/jest": "^27.4.0",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/koa": "^2.13.4",
    "@types/koa-bodyparser": "^4.3.5",
    "@types/koa-helmet": "^6.0.4",
    "@types/koa__cors": "^3.1.1",
    "@types/koa__router": "^8.0.11",
    "@types/mongodb": "^4.0.7",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^17.0.18",
    "@types/socket.io": "^3.0.2",
    "@types/speakingurl": "^13.0.3",
    "@types/winston": "^2.4.4",
    "@types/ws": "^8.2.2",
    "axios-mock-adapter": "^1.20.0",
    "jest": "^27.5.1",
    "jest-extended": "^2.0.0",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "supertest": "^6.2.2",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.5.0",
    "typescript": "^4.5.5"
  },
  "jest": {
    "testEnvironment": "node"
  }
}
